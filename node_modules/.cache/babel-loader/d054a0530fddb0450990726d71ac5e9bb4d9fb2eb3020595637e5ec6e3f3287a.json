{"ast":null,"code":"import { startCoordinator, getCoordinatorStatus } from '@/api/coordinator';\nimport StatusBadge from '@/components/StatusBadge.vue';\nexport default {\n  name: 'CoordinatorDashboard',\n  components: {\n    StatusBadge\n  },\n  data() {\n    return {\n      participantCount: 1,\n      dataSplitType: 'horizontal',\n      status: null\n    };\n  },\n  methods: {\n    async submitStart() {\n      try {\n        const res = await startCoordinator({\n          participant_count: this.participantCount,\n          data_split_type: this.dataSplitType\n        });\n        alert('启动成功: ' + res.data.message);\n        await this.fetchStatus();\n      } catch (e) {\n        alert('启动失败: ' + (e.response?.data?.message || e.message));\n      }\n    },\n    async fetchStatus() {\n      try {\n        const res = await getCoordinatorStatus();\n        this.status = res.data;\n      } catch (e) {\n        console.error('获取状态失败:', e);\n      }\n    }\n  },\n  mounted() {\n    this.fetchStatus();\n  }\n};","map":{"version":3,"names":["startCoordinator","getCoordinatorStatus","StatusBadge","name","components","data","participantCount","dataSplitType","status","methods","submitStart","res","participant_count","data_split_type","alert","message","fetchStatus","e","response","console","error","mounted"],"sources":["D:\\down\\MPHEDEV\\encryption_co\\src\\views\\CoordinatorDashboard.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin-bottom: 20px;\">\n      <label>\n        参与方数量：\n        <input v-model.number=\"participantCount\" type=\"number\" min=\"1\" style=\"width: 60px;\" />\n      </label>\n      <label style=\"margin-left: 20px;\">\n        数据集划分方式：\n        <select v-model=\"dataSplitType\">\n          <option value=\"horizontal\">horizontal</option>\n          <option value=\"vertical\">vertical</option>\n        </select>\n      </label>\n      <button @click=\"submitStart\">启动协调器</button>\n    </div>\n    \n    <h2>协调器状态</h2>\n    <div v-if=\"status\" class=\"status-info\">\n      <div class=\"status-item\">\n        <strong>协调器ID:</strong> {{ status.coordinator_id }}\n      </div>\n      <div class=\"status-item\">\n        <strong>预期参与方数量:</strong> {{ status.expected_participants }}\n      </div>\n      <div class=\"status-item\">\n        <strong>数据集划分方式:</strong> {{ status.data_split_type }}\n      </div>\n      <div class=\"status-item\">\n        <strong>状态:</strong> <StatusBadge :status=\"status.status\" />\n      </div>\n      <div class=\"status-item\">\n        <strong>协调器IP:</strong> {{ status.coordinator_ip }}\n      </div>\n      <div class=\"status-item\">\n        <strong>协调器端口:</strong> {{ status.coordinator_port }}\n      </div>\n      <div class=\"status-item\">\n        <strong>启动时间:</strong> {{ status.start_time }}\n      </div>\n    </div>\n    \n    <div v-else class=\"no-status\">\n      未启动协调器，请先设置参数并启动\n    </div>\n  </div>\n</template>\n\n<script>\nimport { startCoordinator, getCoordinatorStatus } from '@/api/coordinator'\nimport StatusBadge from '@/components/StatusBadge.vue'\n\nexport default {\n  name: 'CoordinatorDashboard',\n  components: { StatusBadge },\n  data() {\n    return {\n      participantCount: 1,\n      dataSplitType: 'horizontal',\n      status: null\n    }\n  },\n  methods: {\n    async submitStart() {\n      try {\n        const res = await startCoordinator({\n          participant_count: this.participantCount,\n          data_split_type: this.dataSplitType\n        });\n        alert('启动成功: ' + res.data.message);\n        await this.fetchStatus();\n      } catch (e) {\n        alert('启动失败: ' + (e.response?.data?.message || e.message));\n      }\n    },\n    async fetchStatus() {\n      try {\n        const res = await getCoordinatorStatus();\n        this.status = res.data;\n      } catch (e) {\n        console.error('获取状态失败:', e);\n      }\n    }\n  },\n  mounted() {\n    this.fetchStatus();\n  }\n}\n</script>\n\n<style scoped>\n.status-info {\n  background: #f5f5f5;\n  padding: 20px;\n  border-radius: 8px;\n  margin-top: 20px;\n}\n.status-item {\n  margin-bottom: 10px;\n}\n.no-status {\n  color: #666;\n  font-style: italic;\n  margin-top: 20px;\n}\n</style>\n"],"mappings":"AAiDA,SAASA,gBAAgB,EAAEC,oBAAmB,QAAS,mBAAkB;AACzE,OAAOC,WAAU,MAAO,8BAA6B;AAErD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAY,CAAC;EAC3BG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,CAAC;MACnBC,aAAa,EAAE,YAAY;MAC3BC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMC,GAAE,GAAI,MAAMX,gBAAgB,CAAC;UACjCY,iBAAiB,EAAE,IAAI,CAACN,gBAAgB;UACxCO,eAAe,EAAE,IAAI,CAACN;QACxB,CAAC,CAAC;QACFO,KAAK,CAAC,QAAO,GAAIH,GAAG,CAACN,IAAI,CAACU,OAAO,CAAC;QAClC,MAAM,IAAI,CAACC,WAAW,CAAC,CAAC;MAC1B,EAAE,OAAOC,CAAC,EAAE;QACVH,KAAK,CAAC,QAAO,IAAKG,CAAC,CAACC,QAAQ,EAAEb,IAAI,EAAEU,OAAM,IAAKE,CAAC,CAACF,OAAO,CAAC,CAAC;MAC5D;IACF,CAAC;IACD,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAML,GAAE,GAAI,MAAMV,oBAAoB,CAAC,CAAC;QACxC,IAAI,CAACO,MAAK,GAAIG,GAAG,CAACN,IAAI;MACxB,EAAE,OAAOY,CAAC,EAAE;QACVE,OAAO,CAACC,KAAK,CAAC,SAAS,EAAEH,CAAC,CAAC;MAC7B;IACF;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACL,WAAW,CAAC,CAAC;EACpB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}