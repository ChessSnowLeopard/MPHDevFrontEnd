{"ast":null,"code":"import { getDetailedStatus } from '@/api/coordinator';\nimport axios from 'axios';\nexport default {\n  name: 'CoordinatorDashboard',\n  data() {\n    return {\n      status: {\n        participantCount: 0,\n        dataSplitType: '',\n        minParticipants: 0,\n        minParticipantsPercent: 0,\n        port: 8080,\n        currentStatus: '等待参与方连接'\n      },\n      participantCount: 1,\n      currentParticipantCount: 0\n    };\n  },\n  mounted() {\n    this.fetchStatus();\n  },\n  methods: {\n    async fetchStatus() {\n      const res = await getDetailedStatus();\n      Object.assign(this.status, res.data);\n    },\n    async submitCount() {\n      try {\n        // TODO: 替换为你的后端接口地址\n        await axios.post('http://127.0.0.1:8080/api/participant_count', {\n          count: this.participantCount\n        });\n        this.currentParticipantCount = this.participantCount;\n        alert('提交成功');\n      } catch (e) {\n        alert('提交失败');\n      }\n    }\n  }\n};","map":{"version":3,"names":["getDetailedStatus","axios","name","data","status","participantCount","dataSplitType","minParticipants","minParticipantsPercent","port","currentStatus","currentParticipantCount","mounted","fetchStatus","methods","res","Object","assign","submitCount","post","count","alert","e"],"sources":["D:\\pass\\encryption_co\\src\\views\\CoordinatorDashboard.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin-bottom: 20px;\">\n      <label>\n        参与方数量：\n        <input v-model.number=\"participantCount\" type=\"number\" min=\"1\" style=\"width: 60px;\" />\n      </label>\n      <button @click=\"submitCount\">提交</button>\n    </div>\n    <h2>协调器仪表盘</h2>\n    <ul>\n      <li>参与方数量: {{ status.participantCount }}</li>\n      <li>数据集划分方式: {{ status.dataSplitType }}</li>\n      <li>最小参与方阈值: {{ status.minParticipants }} ({{ status.minParticipantsPercent }}%)</li>\n      <li>监听端口: {{ status.port }}</li>\n      <li>当前状态: {{ status.currentStatus }}</li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { getDetailedStatus } from '@/api/coordinator'\nimport axios from 'axios'\n\nexport default {\n  name: 'CoordinatorDashboard',\n  data() {\n    return {\n      status: {\n        participantCount: 0,\n        dataSplitType: '',\n        minParticipants: 0,\n        minParticipantsPercent: 0,\n        port: 8080,\n        currentStatus: '等待参与方连接'\n      },\n      participantCount: 1,\n      currentParticipantCount: 0\n    }\n  },\n  mounted() {\n    this.fetchStatus()\n  },\n  methods: {\n    async fetchStatus() {\n      const res = await getDetailedStatus()\n      Object.assign(this.status, res.data)\n    },\n    async submitCount() {\n      try {\n        // TODO: 替换为你的后端接口地址\n        await axios.post('http://127.0.0.1:8080/api/participant_count', {\n          count: this.participantCount\n        })\n        this.currentParticipantCount = this.participantCount\n        alert('提交成功')\n      } catch (e) {\n        alert('提交失败')\n      }\n    }\n  }\n}\n</script>\n"],"mappings":"AAqBA,SAASA,iBAAgB,QAAS,mBAAkB;AACpD,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,gBAAgB,EAAE,CAAC;QACnBC,aAAa,EAAE,EAAE;QACjBC,eAAe,EAAE,CAAC;QAClBC,sBAAsB,EAAE,CAAC;QACzBC,IAAI,EAAE,IAAI;QACVC,aAAa,EAAE;MACjB,CAAC;MACDL,gBAAgB,EAAE,CAAC;MACnBM,uBAAuB,EAAE;IAC3B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAG;MAClB,MAAME,GAAE,GAAI,MAAMf,iBAAiB,CAAC;MACpCgB,MAAM,CAACC,MAAM,CAAC,IAAI,CAACb,MAAM,EAAEW,GAAG,CAACZ,IAAI;IACrC,CAAC;IACD,MAAMe,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF;QACA,MAAMjB,KAAK,CAACkB,IAAI,CAAC,6CAA6C,EAAE;UAC9DC,KAAK,EAAE,IAAI,CAACf;QACd,CAAC;QACD,IAAI,CAACM,uBAAsB,GAAI,IAAI,CAACN,gBAAe;QACnDgB,KAAK,CAAC,MAAM;MACd,EAAE,OAAOC,CAAC,EAAE;QACVD,KAAK,CAAC,MAAM;MACd;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}